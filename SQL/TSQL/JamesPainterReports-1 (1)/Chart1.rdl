  <DataSets>
    <DataSet Name="Hotel_Room_Revenue">
      <Query>
        <DataSourceName>Painter_FARMS</DataSourceName>
        <CommandText>SELECT rm.RoomID, rm.RoomNumber, h.HotelName,COUNT(h.HotelName) AS [ROOM COUNT],SUM(F.QuotedRate) AS [TOTAL]
FROM FOLIO f
JOIN ROOM rm 
ON f.RoomID = rm.RoomID
JOIN HOTEL h 
ON rm.HotelID = h.HotelID
GROUP BY rm.roomID, rm.RoomNumber,h.HotelName
ORDER BY  [TOTAL] DESC
</CommandText>
      </Query>
      <Fields>
        <Field Name="RoomID">
          <DataField>RoomID</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="RoomNumber">
          <DataField>RoomNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="HotelName">
          <DataField>HotelName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ROOM_COUNT">
          <DataField>ROOM COUNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TOTAL">
          <DataField>TOTAL</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
