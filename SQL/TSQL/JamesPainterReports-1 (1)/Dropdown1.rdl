  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Painter_FARMS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@NAME">
            <Value>=Parameters!NAME.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT (GuestFirstName + ' ' + GuestLastName) as [NAME], sum(f.QuotedRate) as [Total]
FROM GUEST g 
JOIN CREDITCARD cc
ON g.GuestID = cc.GuestID
JOIN RESERVATION r 
ON cc.CreditCardID = r.CreditCardID 
JOIN FOLIO f 
ON r.reservationID = f.ReservationID
WHERE (GuestFirstName + ' ' + GuestLastName) = @NAME 
GROUP BY (GuestFirstName + ' ' + GuestLastName)</CommandText>
      </Query>
      <Fields>
        <Field Name="NAME">
          <DataField>NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="DataSet2">
      <Query>
        <DataSourceName>Painter_FARMS</DataSourceName>
        <CommandText>SELECT DISTINCT (GuestFirstName + ' ' + GuestLastName) as [NAME] FROM GUEST</CommandText>
      </Query>
      <Fields>
        <Field Name="NAME">
          <DataField>NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
